//Inorder Traversal 
void inOrder(Node *root) {
        if(root==NULL) return;
        inOrder(root->left);
        cout<<root->data<<" ";
        inOrder(root->right);
}


//Preorder Traversal
void preOrder(Node *root) {
        if(root==NULL) return;
        cout<<root->data<<" ";
        preOrder(root->left);
        preOrder(root->right);
}

//Postorder Traversal
void postOrder(Node *root) {
        if(root==NULL) return;
        postOrder(root->left);
        postOrder(root->right);
        cout<<root->data<<" ";
        
}
