
#include <bits/stdc++.h>
using namespace std;
int main(){
    vector<int> A(3);
    vector<int> B(3);
    int cA=0;
    int cB=0;
    for(int i=0;i<3;i++){
        
        cin>>A[i];
        
    }
    for(int i=0;i<3;i++){
        
        cin>>B[i];
        
    }
    
    for(int i=0;i<3;i++){
        if(A[i]>B[i]) cA++;
        else if (A[i]<B[i]) cB++;
        else continue;
    }
    
    cout<<cA<<" "<<cB<<endl;
    return 0;
}
